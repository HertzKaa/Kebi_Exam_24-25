@prefix : <http://example.org/pm#> .
@prefix pm: <http://example.org/pm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/pm#> .

<http://example.org/pm#> rdf:type owl:Ontology ;
                          rdfs:label "Personalized Menu Ontology v3" .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/pm#GuestHasFoodType
pm:GuestHasFoodType rdf:type owl:ObjectProperty ;
                    rdfs:domain pm:Guest ;
                    rdfs:range pm:GuestFoodTypePreference .


###  http://example.org/pm#IngredientNotCompatibleWithFoodTypePreference
pm:IngredientNotCompatibleWithFoodTypePreference rdf:type owl:ObjectProperty ;
                                                 rdfs:domain pm:Ingredient ;
                                                 rdfs:range pm:GuestFoodTypePreference .


###  http://example.org/pm#IngredientUsageInMeal
pm:IngredientUsageInMeal rdf:type owl:ObjectProperty ;
                         rdfs:domain pm:IngredientUsage ;
                         rdfs:range pm:Meal .


###  http://example.org/pm#MealNotCompatibleWithFoodPreference
pm:MealNotCompatibleWithFoodPreference rdf:type owl:ObjectProperty ;
                                       rdfs:domain pm:Meal ;
                                       rdfs:range pm:GuestFoodPreference .


###  http://example.org/pm#UsedIngredient
pm:UsedIngredient rdf:type owl:ObjectProperty ;
                  rdfs:domain pm:IngredientUsage ;
                  rdfs:range pm:Ingredient .


#################################################################
#    Data properties
#################################################################

###  http://example.org/pm#IngredientHasCalories
pm:IngredientHasCalories rdf:type owl:DatatypeProperty ;
                         rdfs:domain pm:Ingredient ;
                         rdfs:range xsd:decimal .


###  http://example.org/pm#IngredientUsageInGrams
pm:IngredientUsageInGrams rdf:type owl:DatatypeProperty ;
                          rdfs:domain pm:IngredientUsage ;
                          rdfs:range xsd:decimal .


###  http://example.org/pm#MealHasCalories
pm:MealHasCalories rdf:type owl:DatatypeProperty ;
                   rdfs:domain pm:Meal ;
                   rdfs:range xsd:decimal .


###  http://example.org/pm#ObjectHasName
pm:ObjectHasName rdf:type owl:DatatypeProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://example.org/pm#Allergen
pm:Allergen rdf:type owl:Class ;
            rdfs:subClassOf pm:GuestFoodTypePreference .


###  http://example.org/pm#Diet
pm:Diet rdf:type owl:Class ;
        rdfs:subClassOf pm:GuestFoodTypePreference .


###  http://example.org/pm#Guest
pm:Guest rdf:type owl:Class .


###  http://example.org/pm#GuestCaloricPreference
pm:GuestCaloricPreference rdf:type owl:Class ;
                          rdfs:subClassOf pm:GuestFoodPreference .


###  http://example.org/pm#GuestFoodPreference
pm:GuestFoodPreference rdf:type owl:Class .


###  http://example.org/pm#GuestFoodTypePreference
pm:GuestFoodTypePreference rdf:type owl:Class ;
                           rdfs:subClassOf pm:GuestFoodPreference .


###  http://example.org/pm#Ingredient
pm:Ingredient rdf:type owl:Class .


###  http://example.org/pm#IngredientUsage
pm:IngredientUsage rdf:type owl:Class .


###  http://example.org/pm#Meal
pm:Meal rdf:type owl:Class .


