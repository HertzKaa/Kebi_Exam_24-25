#################################################################
#    Rules
#################################################################

pm:usage rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

pm:meal rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

pm:ingredient rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
   rdfs:comment "Rule that checks if a meal is not compatible with Lactose Intolerant guests" ;
   rdfs:label "Rule2" ;
   rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> pm:IngredientUsageInMeal ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> pm:usage ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> pm:meal
